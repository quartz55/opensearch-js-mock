{
  "name": "opensearch-mock",
  "version": "0.3.1",
  "description": "Mock utility for the OpenSearch's Node.js client",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "standard && ava -v && tsd",
    "coverage:report": "nyc ava && nyc report --reporter=lcov && echo \"\n==> open coverage/lcov-report/index.html\"",
    "coverage:check": "nyc ava && nyc check-coverage --branches=100 --lines=100 --functions=100 --statements=100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quartz55/opensearch-js-mock.git"
  },
  "keywords": [
    "elasticsearch",
    "elastic",
    "kibana",
    "mapping",
    "REST",
    "search",
    "client",
    "index",
    "mock",
    "test",
    "stub",
    "sinon"
  ],
  "author": "Tomas Della Vedova",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/quartz55/opensearch-js-mock/issues"
  },
  "homepage": "https://github.com/quartz55/opensearch-js-mock#readme",
  "devDependencies": {
    "@opensearch-project/opensearch": "^1.2.0",
    "ava": "^5.3.1",
    "nyc": "^15.1.0",
    "standard": "^17.1.0",
    "tsd": "^0.28.1"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "find-my-way": "^7.6.2"
  }
